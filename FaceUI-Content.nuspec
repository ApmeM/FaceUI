<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>FaceUI.Content</id>
    <version>1.0.0</version>
    <title>FaceUI.Content</title>
    <authors>ApmeM</authors>
    <owners>ApmeM</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>FaceAI.Content is a set of predefined content objects with default values to work with FaceUI.</description>
    <copyright>Copyright © ApmeM 2019</copyright>
    <dependencies>
      <group targetFramework="netstandard2.0">
      </group>
    </dependencies>
    <repository type="git" url="https://github.com/ApmeM/FaceUI.git" /> 
  </metadata>
  <files>
    <file src="FaceUI\Content\FaceUI\**\*.*" target="nugetData\Content\FaceUI" /> 
    <file src="FaceUI\Content\lib\*.dll" target="nugetData\Content\lib" />                        
    <file src="FaceUI\Content\FaceUIContent.mgcb" target="nugetData\Content\FaceUIContent.mgcb" />
    <file src="FaceUI-Content.targets" target="build\FaceUI.Content.targets" /> 
  </files>
</package>
